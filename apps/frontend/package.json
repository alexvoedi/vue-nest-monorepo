{
  "name": "frontend",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "serve": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --run",
    "test:cov": "vitest --coverage",
    "test:watch": "vitest --run",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.1",
    "common": "workspace:*",
    "pinia": "^2.2.1",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.68",
    "@types/node": "^22.2.0",
    "@unocss/preset-attributify": "^0.61.9",
    "@unocss/preset-icons": "^0.61.9",
    "@unocss/preset-uno": "^0.61.9",
    "@unocss/reset": "^0.61.9",
    "@unocss/transformer-directives": "^0.61.9",
    "@unocss/transformer-variant-group": "^0.61.9",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/compiler-sfc": "^3.4.37",
    "cross-env": "^7.0.3",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "tsconfig": "workspace:*",
    "typescript": "^5.5.4",
    "unocss": "^0.61.9",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.4.0",
    "vite-plugin-inspect": "^0.8.5",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,json,html}": "eslint --fix"
  }
}
